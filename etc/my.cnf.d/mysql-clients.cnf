#
# These groups are read by MariaDB command-line tools
# Use it for options that affect only one utility
#

[mysql]
no-auto-rehash
default-character-set   = utf8
prompt='\\u:[\\d@\\h][\\R:\\m:\\s]> ' 

[mysql_upgrade]

[mysqladmin]

[mysqlbinlog]

[mysqlcheck]

[mysqldump]
max_allowed_packet      = 1G

[mysqlimport]

[mysqlshow]

[mysqlslap]
